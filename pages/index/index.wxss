page {
	background: #C5EFFF;
}
.contianer {
	display: flex;
	flex-direction: column;
	align-items: center;

}

.contianer image {
	width: 200rpx;
	height: 200rpx;
	margin-top: 160rpx;
	border-radius: 100rpx;
}

.userName {
	font-size: 32rpx;
